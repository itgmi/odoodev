<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="product_pricelist_view_custom_gr" model="ir.ui.view">
        <field name="name">product.pricelist.view.custom.gr</field>
        <field name="model">product.pricelist</field>
        <field name="inherit_id"
               ref="product.product_pricelist_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='pricelist_settings']" position="after">
                <group>
                    <field name="partner_id"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>