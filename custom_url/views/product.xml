<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="product_temp_view_custom" model="ir.ui.view">
        <field name="name">product.temp.view.custom</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="custom_product.product_temp_view_custom"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='image_url']" position="after">
                <field name="public_url_sp"/>
            </xpath>
        </field>
    </record>
</odoo>