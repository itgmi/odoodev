<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="channels_gr_model_tree" model="ir.ui.view">
        <field name="name">channels gr</field>
        <field name="model">channels.gr</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>
     <record id="channels_gr_model_action" model="ir.actions.act_window">
        <field name="name">Channels</field>
        <field name="res_model">channels.gr</field>
        <field name="view_mode">tree</field>
    </record>
    <menuitem id="menu_gr_channels"
              name="Channels"
              parent="sale.menu_sale_config"
              sequence="110"
              action="channels_gr_model_action"/>
</odoo>