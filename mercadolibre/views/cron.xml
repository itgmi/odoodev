<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="cron_refresh_view_custom" model="ir.ui.view">
        <field name="name">cron.refresh.view.custom</field>
        <field name="model">ir.cron</field>
        <field name="inherit_id" ref="base.ir_cron_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[last()]" position="inside">
                <page string="Refresh Token Logs" name="logs">
                    <group>
                        <field name="refresh_logs"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>